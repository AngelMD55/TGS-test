{
  "name": "tgs-test",
  "version": "1.0.0",
  "description": "test for TGS single page app log in page",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AngelMD55/TGS-test.git"
  },
  "author": "Angel Demerutis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AngelMD55/TGS-test/issues"
  },
  "homepage": "https://github.com/AngelMD55/TGS-test#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "concurrently": "^4.1.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  }
}
